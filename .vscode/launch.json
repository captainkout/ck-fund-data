  "version": "0.2.0",
  "configurations": [
    {
      "name": "l-getdata",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-getdata",
      "program": "${workspaceFolder}/GetData/bin/Debug/net6.0/GetData.dll",
      "args": [],
      "cwd": "${workspaceFolder}/GetData",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "l-processdata",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-processdata",
      "program": "${workspaceFolder}/ProcessData/bin/Debug/net6.0/ProcessData.dll",
      "args": ["--r1=RPBAX", "--r2=KTRAX"],
      "cwd": "${workspaceFolder}/ProcessData",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
